<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Login.MainActivity"
    android:background="#f0b67f">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nombre"
        android:id="@+id/txtCliente"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/button16"
        android:layout_alignStart="@+id/button16" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Bienvenido"
        android:id="@+id/textView2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="49dp"
        android:layout_marginStart="49dp"
        android:background="#fc8321" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Categorias"
        android:id="@+id/textView3"
        android:background="#f3a600"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/button"
        android:layout_alignStart="@+id/button" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Desayuno"
        android:id="@+id/button"
        android:drawableRight="@drawable/abc_btn_rating_star_on_mtrl_alpha"
        android:background="#f58549"
        android:layout_marginBottom="65dp"
        android:layout_above="@+id/button2"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Almuerzo"
        android:id="@+id/button2"
        android:drawableRight="@drawable/abc_btn_rating_star_on_mtrl_alpha"
        android:background="#f58549"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/button"
        android:layout_alignStart="@+id/button" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cena"
        android:id="@+id/button3"
        android:drawableRight="@drawable/abc_btn_rating_star_on_mtrl_alpha"
        android:layout_below="@+id/button2"
        android:layout_marginTop="63dp"
        android:background="#f58549"
        android:layout_alignLeft="@+id/button2"
        android:layout_alignStart="@+id/button2"
        android:layout_alignRight="@+id/button2"
        android:layout_alignEnd="@+id/button2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eventos"
        android:id="@+id/button14"
        android:drawableRight="@drawable/abc_ic_menu_paste_mtrl_am_alpha"
        android:background="#1e8bbd"
        android:layout_alignTop="@+id/button"
        android:layout_alignLeft="@+id/button15"
        android:layout_alignStart="@+id/button15"
        android:layout_alignRight="@+id/button15"
        android:layout_alignEnd="@+id/button15" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Localizacion"
        android:id="@+id/button15"
        android:drawableRight="@drawable/abc_ic_search_api_mtrl_alpha"
        android:layout_above="@+id/button3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#1e8bbd" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salones"
        android:id="@+id/button16"
        android:drawableRight="@android:drawable/ic_dialog_map"
        android:layout_alignTop="@+id/button3"
        android:layout_alignRight="@+id/button15"
        android:layout_alignEnd="@+id/button15"
        android:background="#1e8bbd"
        android:layout_alignLeft="@+id/button15"
        android:layout_alignStart="@+id/button15" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Otros"
        android:id="@+id/textView49"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#e4a444" />

</RelativeLayout>
